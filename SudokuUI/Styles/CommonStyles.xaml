<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="cell_border_style" TargetType="Border">
        <Setter Property="BorderBrush" Value="{DynamicResource MahApps.Brushes.Accent}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Padding" Value="4"/>
        <Setter Property="Width" Value="50"/>
        <Setter Property="Height" Value="50"/>
    </Style>

    <SolidColorBrush x:Key="cell_highlight_color" Color="CornflowerBlue"/>
    <SolidColorBrush x:Key="cell_positive_color" Color="ForestGreen"/>
    <SolidColorBrush x:Key="cell_negative_color" Color="IndianRed"/>
    <SolidColorBrush x:Key="cell_information_color" Color="Yellow"/>
    <SolidColorBrush x:Key="cell_background_color" Color="Transparent"/>

    <SolidColorBrush x:Key="strong_link_color" Color="Blue"/>
    <SolidColorBrush x:Key="weak_link_color" Color="Red"/>
</ResourceDictionary>